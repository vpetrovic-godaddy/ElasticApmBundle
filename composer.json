{
    "name": "spacespell/elastic-apm-bundle",
    "description": "Elastic APM Bundle",
    "type": "symfony-bundle",
    "homepage": "https://github.com/spacespell/ElasticApmBundle",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vpetrovic-godaddy/ElasticApmBundle"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.6|~3.0",
        "philkra/elastic-apm-php-agent": "dev-master#84a5f6c",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0@dev"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Phoenix Gao",
            "email": "phoenix@spacespell.com"
        },
        {
            "name": "Veljko Petrovic",
            "email": "vpetrovic@godaddy.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SpaceSpell\\ElasticApmBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaceSpell\\Tests\\ElasticApmBundle\\": "tests/"
        }
    }
}
